<h1>音楽オススメサイト</h1>
<h3>編集</h3>
<%= form_for @music do |f| %>
  <div class="field">
    <%= f.label :アーティスト名 %>
    <%= f.text_field :artist_name, :size => 140 %>
    <%= f.label :曲名 %>
    <%= f.text_field :song_name, :size => 140 %>
  </div>
  <%= f.submit "決定" %>
<% end %>
<%= link_to 'トップ', :controller => "musics", :action => "index" %>